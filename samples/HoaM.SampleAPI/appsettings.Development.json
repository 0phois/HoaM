{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityOptions": {
    "Password": {
      "RequireDigit": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireNonAlphanumeric": false,
      "RequiredUniqueChars": 3,
      "RequiredLength": 6
    }
  },
  "DatabaseOptions": {
    "ProtectSensitiveData": false,
    "ProviderType": "SQLite",
    "ConnectionString": "Data Source=sample.db",
    "MigrationsAssembly": "HoaM.SampleAPI"
  }
}
